#include <iostream>
#include <string>
using namespace std;
int main() {
	setlocale(LC_ALL, "");
		const string correct_password = "12345";
		string input_password;
		
		cout << "проверка пароля\nвведите пароль: ";
		cin >> input_password;

		while (input_password != correct_password) {
			cout << "НЕверный пароль, попробуй еще раз\n";
			cin >> input_password;
		}
		cout << "дорова";
		
		return 0;
}
