#include <iostream>;
using namespace std;
int main() {
	setlocale(LC_ALL, "");
	int multiplier;
	int counter = 1;
	int result;
	cout << "таблица умножения\n";
	cout << "введи число\n";
		if (!(cin >> multiplier)) {
			return 1;
		}
		cout << "таблица умножения для числа " << multiplier << endl;
		while (counter <= 10) {
			result = multiplier * counter;
			cout << multiplier << " * " << counter << " = " << result << endl;
			counter++;
		}
		return 0;
}
